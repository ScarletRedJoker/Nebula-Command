modules = ["python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0", "spotify:1.0.0", "google-calendar:1.0.0", "google-mail:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cacert", "iana-etc", "libyaml", "unzip", "xcodebuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "stream-bot-dev"

[[workflows.workflow]]
name = "stream-bot-dev"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/stream-bot && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 24678
externalPort = 3001

[[ports]]
localPort = 36017
externalPort = 3003

[[ports]]
localPort = 46877
externalPort = 3000
