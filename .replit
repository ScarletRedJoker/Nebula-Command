modules = ["python-3.11", "nodejs-20", "postgresql-16"]
[agent]
expertMode = true
integrations = ["python_openai_ai_integrations:1.0.0", "spotify:1.0.0", "google-calendar:1.0.0", "google-mail:1.0.0", "discord:1.0.0", "javascript_openai_ai_integrations:1.0.0", "youtube:1.0.0", "github:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["cacert", "cargo", "iana-etc", "libiconv", "libxcrypt", "libyaml", "openssl", "pkg-config", "rustc", "sshpass", "unzip", "xcodebuild", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "discord-bot"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "dashboard"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "stream-bot"

[[workflows.workflow]]
name = "discord-bot"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/discord-bot && NODE_ENV=development npm run dev"

[[workflows.workflow]]
name = "dashboard"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/dashboard && python main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "stream-bot"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd services/stream-bot && PORT=3000 NODE_ENV=development npm run dev"
waitForPort = 3000

